{
  "name": "sizecostimport",
  "displayName": "Size-Cost-Import",
  "description": "Display the size of imported packages inline.",
  "icon": "dist/sci_icon.png",
  "keywords": [
    "bundle-size",
    "import-cost",
    "package-size",
    "npm",
    "javascript",
    "typescript",
    "performance",
