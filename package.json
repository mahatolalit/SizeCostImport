    "compile-esbuild": "node ./build.js",
    "watch-esbuild": "node ./build.js --watch",
    "pack": "vsce package"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@types/vscode": "^1.80.0",
    "@vscode/vsce": "^3.7.1",
