    "size"
  ],
  "categories": [
    "Visualization",
    "Other"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/mahatolalit/SizeCostImport"
  },
  "version": "0.0.6",
  "publisher": "mahatolalit",
  "author": {
    "name": "Lalit Mahato",
    "url": "https://github.com/mahatolalit"
  },
  "license": "MIT",
  "funding": {
    "type": "ko-fi",
    "url": "https://ko-fi.com/lalitmahato"
  },
  "engines": {
    "vscode": "^1.80.0"
  },
  "activationEvents": [
    "onLanguage:javascript",
    "onLanguage:typescript",
    "onLanguage:javascriptreact",
    "onLanguage:typescriptreact"
  ],
  "main": "./dist/extension.js",
  "contributes": {},
  "scripts": {
    "vscode:prepublish": "node ./build.js --production",
